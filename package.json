{
  "name": "@fjell/sample-app",
  "version": "0.0.1-dev.0",
  "description": "Sample Fjell Express Application with Widget and WidgetType models",
  "keywords": [
    "fjell",
    "express",
    "sample",
    "sequelize",
    "reference"
  ],
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "type": "module",
  "engines": {
    "node": ">=21"
  },
  "scripts": {
    "build": "npm run lint && node build.js && node build-client.js",
    "build:server": "npm run lint && node build.js",
    "build:client": "node build-client.js",
    "dev": "npm run build && node dist/index.js",
    "start": "node dist/index.js",
    "lint": "eslint . --ext .ts --fix",
    "clean": "rm -rf dist",
    "test": "npm run lint && vitest run --coverage",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "dependencies": {
    "@fjell/cache": "file:../fjell-cache",
    "@fjell/client-api": "file:../fjell-client-api",
    "@fjell/core": "file:../fjell-core",
    "@fjell/express-router": "file:../fjell-express-router",
    "@fjell/http-api": "file:../fjell-http-api",
    "@fjell/lib": "file:../fjell-lib",
    "@fjell/lib-sequelize": "file:../fjell-lib-sequelize",
    "@fjell/logging": "file:../fjell-logging",
    "@fjell/providers": "file:../fjell-providers",
    "@fjell/registry": "file:../fjell-registry",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@types/react-router-dom": "^5.3.3",
    "@types/supertest": "^6.0.3",
    "express": "^5.1.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.8.0",
    "sequelize": "^6.37.7",
    "sqlite3": "^5.1.7",
    "supertest": "^7.1.4",
    "util": "^0.12.5"
  },
  "devDependencies": {
    "@fjell/eslint-config": "file:../fjell-eslint-config",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@tsconfig/recommended": "^1.0.10",
    "@types/express": "^5.0.3",
    "@types/node": "^24.1.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitest/coverage-v8": "^3.2.4",
    "esbuild": "^0.25.8",
    "eslint": "^9.32.0",
    "jsdom": "^26.1.0",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  }
}
